# Environment Variables for Critical Weight Analysis Tool
# Copy this file to .env and set your values

# Hugging Face Authentication
HF_TOKEN=hf_XXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXX

# CUDA Configuration (optional)
CUDA_VISIBLE_DEVICES=0
CUDA_CACHE_PATH=/tmp/cuda_cache

# Model Cache Configuration
HF_HOME=/tmp/hf_cache
TRANSFORMERS_CACHE=/tmp/hf_cache
HF_DATASETS_CACHE=/tmp/hf_cache

# Lambda Labs Optimizations
PYTORCH_CUDA_ALLOC_CONF=max_split_size_mb:512
TOKENIZERS_PARALLELISM=false

# Logging Configuration
LOG_LEVEL=INFO
CWA_OUTPUT_DIR=outputs

# Optional: Weights & Biases (for experiment tracking)
# WANDB_API_KEY=your_wandb_key_here
# WANDB_PROJECT=critical-weight-analysis

# Optional: Model-specific settings
# FLASH_ATTENTION_ENABLED=true
# QUANTIZATION_DEFAULT=8bit